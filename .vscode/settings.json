{
    "runOnSave.commands": [
        {
            "match": "data\\\\contests\\.yaml",
            "command": "node .vscode\\yamlToJson.js ${file} ${workspaceFolder}\\assets\\data\\${fileBasenameNoExtension}.json",
            "runIn": "backend",
            "runningStatusMessage": "jsonify ${fileBasenameNoExtension}",
        }
    ]
}